{
  "name": "gulp-json-loader",
  "description": "A little tool for the gulp-data plugin. It useful for data loading in the development of HTML or the pug pages",
  "author": "Alexander Yukal <yukal@email.ua>",
  "version": "1.2.1",
  "main": "gulpfile.js",
  "devDependencies": {
    "chai": "^5.0.0",
    "gulp": "^4.0.2",
    "gulp-data": "^1.3.1",
    "gulp-load-plugins": "^2.0.8",
    "gulp-pug": "^5.0.0",
    "mocha": "^10.2.0"
  },
  "scripts": {
    "test": "mocha --no-config --no-package ./test/**/test*.js"
  },
  "gulp-json-loader": {
    "sourcePath": "src",
    "dataEntry": "$",
    "locales": "ru-UA",
    "report": true
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yukal/gulp-json-loader.git"
  },
  "keywords": [
    "gulp",
    "json load",
    "json data"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yukal/gulp-json-loader/issues"
  },
  "homepage": "https://github.com/yukal/gulp-json-loader#readme"
}
